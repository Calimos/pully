{
  "name": "pully",
  "version": "1.1.0",
  "description": "A very simple HD YouTube downloader.",
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBoh/pully.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover -x \"*.spec.js\" --print detail ./node_modules/jasmine-node/bin/jasmine-node spec && codeclimate-test-reporter < ./coverage/lcov.info",
    "coverage-all": "istanbul cover --print detail ./node_modules/jasmine-node/bin/jasmine-node spec",
    "lint": "eslint -c ./.eslintrc --fix ./"
